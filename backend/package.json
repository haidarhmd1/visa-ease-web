{
  "name": "visastar-backend",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.ts",
  "engines": {
    "node": ">=18.12.1 <19.0.0"
  },
  "scripts": {
    "docker:up": "docker compose up",
    "docker:down": "docker compose down",
    "start": "node build/src/index.js",
    "build": "tsc -p ./tsconfig.build.json && tsconfig-replace-paths -p ./tsconfig.build.json",
    "dev": "ts-node-dev -r tsconfig-paths/register --exit-child --respawn --files src/index.ts | pino-pretty --colorize",
    "debug": "ts-node-dev --inspect=0.0.0.0:9229 -r tsconfig-paths/register --respawn --files src/index.ts | pino-pretty --colorize",
    "migrate-dev": "yarn install-dependencies && yarn dev",
    "ts:check": "tsc -p . --noEmit",
    "lint": "eslint ./ --ext .ts,.tsx src",
    "install-dependencies": "yarn install --check-files --frozen-lockfile --mutex file:/usr/local/share/.cache/yarn/.yarn-mutex; prisma generate;",
    "migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@hapi/boom": "10.0.0",
    "@prisma/client": "^4.12.0",
    "@sendgrid/mail": "^7.7.0",
    "@tsconfig/node18": "1.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "4.18.2",
    "express-async-errors": "3.1.1",
    "http-status": "1.6.2",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "pino-pretty": "9.1.1",
    "prisma": "4.9.0",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "1.19.2",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "4.17.15",
    "@types/express-fileupload": "^1.4.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "18.11.18",
    "@types/node-cron": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "8.32.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-security": "1.5.0",
    "eslint-plugin-security-node": "1.1.1",
    "eslint-plugin-sonarjs": "0.16.0",
    "eslint-plugin-unicorn": "45.0.0",
    "prettier": "2.8.3",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.1.2",
    "tsconfig-replace-paths": "0.0.11",
    "typescript": "4.9.4"
  }
}
